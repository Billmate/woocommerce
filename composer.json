{
  "name": "billmate/billmate-payment-gateway-for-woocommerce",
  "description": "An eCommerce toolkit that helps you sell anything. Beautifully.",
  "homepage": "https://woocommerce.com/",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "wp-coding-standards/wpcs": "^2.2"
  },
  "scripts": {
    "php:codesniffer": "vendor/bin/phpcs src/ vendor/wp-coding-standards/wpcs/",
    "php:unit-tests": "vendor/bin/phpunit Tests/",
    "tests:all": "composer php:codesniffer && composer php:unit-tests"
  }
}
